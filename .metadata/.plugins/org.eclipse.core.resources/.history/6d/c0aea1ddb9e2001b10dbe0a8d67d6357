package hw3Q3JavaBasics;

public class MyAddress02 {

	static String MyName = "Bonny Hewage";
	static int houseNumber = 2920;
	static String StreetName = "Hone Ave";
	static String CityName = "Bronx";
	static String StateName = "NY";
	static int zipCode = 10469;
	static boolean inNY = true;

	public static void main(String[] args) {
		System.out.println(MyName);
		System.out.println(houseNumber);
		System.out.println(StreetName);
		System.out.println(CityName);
		System.out.println(StateName);
		System.out.println(zipCode);
		System.out.println(inNY);

	}

}
