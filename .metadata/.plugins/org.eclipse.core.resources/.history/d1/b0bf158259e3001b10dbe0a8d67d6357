package hw4Q3JavaVariablesConstructorAndMethod;

public class TestMyProfile {

	public static void main(String[] args) {
		MyProfile bonny = new MyProfile();
		bonny.MyName = "Bonny";
		char.mySex = 'M';
		int myAge = 37;
		boolean usCitizen = true;
		byte yearsWork = 15;
		short myHouseRent = 3000;
		long mySalary = 125000;
		float myClassGrade = 3.625f;
		double myHight = 1.5206213;
		bonny.myProfile();
		
		
		
		
		
		MyProfile jhon = new MyProfile();
		jhon.MyName = "Jhon";
		jhon.myProfile();

	}

}
