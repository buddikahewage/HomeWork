package hw3Q3JavaBasics;

public class MyAddress03 {
	
	
	
	static String MyName = "\tBonny Hewage";
	static int houseNumber = \t2920;
	static String streetName = "\tHone Ave";
	static String cityName = "\tBronx";
	static String stateName = "\tNY";
	static int zipCode = \t10469;
	static boolean inNY = \ttrue;

	public static void main(String[] args) {
		
		System.err.println(MyName);
		System.out.println(houseNumber);
		System.out.println(streetName);
		System.out.println(cityName);
		System.out.println(stateName);
		System.out.println(zipCode);
		System.out.println(inNY);
		
		
		
		
		
		
		
		

	}

}
