package hw4Q3JavaVariablesConstructorAndMethod;

public class MyProfile {
	public String MyName; 
	public char mySex;
	public int myAge;
	public boolean usCitizen;
	public byte yearsWork;
	public short myHouseRent;
	public long mySalary;
	public float myClassGrade;
	public double myHight;

	public MyProfile() {
		System.out.println();
	}

	public void myProfile() {
		System.out.println("\t" + "My Name Is:" + MyName + " My Sex Is:" + mySex + " My Age Is:" + myAge
				+ " I Am A US Citizen:" + usCitizen + " I Worked:" + yearsWork + " My House Rent Is:" + myHouseRent
				+ " My Yearly Salary Is:" + mySalary + "My College Grade Is:" + myClassGrade + " My hight Is :"
				+ myHight);

		
	}

}
